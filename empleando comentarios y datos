public static void main(String[] args){


        // tipo de identificador = 30;
        // tipo de indentificador;
        // idneitificador = 30;


        //enteros
        byte number1 = 1; // 1 byte
        short number2 = 2; // 2 bytes
        int number3 = 3; // 4 bytes
        long number4 = 4l; // 8 bytes

        // puntos flotante
        float decimal1 = 4.9f;
        double decimal2 = 4.99;

        //caracter
        char caracter1 = 'a';

        // booleanos
        boolean verdadero = true;
        boolean falso = false;

       /* hasta aqui los datos primitivos */

        // cadenas de texto
        String nombre = "Alan";
        String apellido = "Sastre";

        // tipos envoltorios

        Integer numero = null;
        Long numero2 = 2L;

    }
}
